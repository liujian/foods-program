    onLoad(options){
      let that = this;
      // 获取openid
      wx.getStorage({
        key: 'openid',
        success: function(res) {
          that.openid = res.data;
          that.$apply();
        }
      })
      //获取个人信息
      wx.getStorage({
        key: 'userInfo',
        success: function(res) {
          that.userInfo = res.data;
          that.$apply();
        }
      })
      var value = wx.getStorageSync('data');
      this.tableNum = value.socketTableNum;
      this.eatFoodNum = value.socketFoodNum;
      if (value) {
        this.getMorePeopleData();
      }
      this.billData = options;
      this.length = Object.keys(options).length;
      this.billType = options.type;
      // this.middlePrice = wepy.$instance.globalData.deliverData.price;
      // this.deliverData = wepy.$instance.globalData.deliverData;
      this.tips = wepy.$instance.globalData.num.tips;
      this.$apply();


      // 连接Socket
      var remindTitle = that.socketOpen ? '正在关闭' : '正在连接';
      wx.showToast({
        title: remindTitle,
        icon: 'loading',
        duration: 10000
      })
      if(!this.socketOpen){
        console.log("准备连接webSocket");
        wx.connectSocket({
          url:'wss://socket.qumatou.com.cn'
        })
        //监听WebSocket错误
        wx.onSocketError(function (res) {
          this.socketOpen = false
          console.log('WebSocket连接打开失败，请检查！')
          this.socktBtnTitle = '连接socket'
          wx.hideToast()
        })
        //监听WebSocket连接打开事件。
        let socketMsgQueue = [];
        let that = this;
        let data = {
          type:'UPDATE_CARTS',
          desktop:123456
        }
        data = JSON.stringify(data);
        wx.onSocketOpen(function (res) {
          console.log('WebSocket连接已打开！')
          wx.hideToast()
          this.socktBtnTitle = '断开socket';
          this.socketOpen = true;
          // console.log(socketMsgQueue)
          for (var i = 0; i < socketMsgQueue.length; i++) {
            this.sendSocketMessage(socketMsgQueue[i])
          }
          socketMsgQueue = []
          wx.sendSocketMessage({
            data: data
          })
          console.log("数据发送成功")
        })
        //监听WebSocket接受到服务器的消息事件
        wx.onSocketMessage(function (res) {
          console.log('收到服务器内容：' + res.data);
          let data = JSON.parse(res.data)
          if(data.message == 'UPDATE'){
            console.log("重新调取接口");
            that.getMorePeopleData();
          }

        })
      }
    }
